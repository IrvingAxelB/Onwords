{
  "manifest_version": 2,
  "name": "Annotator",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bower_components/jquery/dist/jquery.min.js", "annotator-firebase.js", "test.js"],
      "css": ["annotator.css","onwords.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "annotator.css",
    "annotator.js"
  ]

}